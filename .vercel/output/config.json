{
  "version": 3,
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "^/$",
      "dest": "/api/index.go",
      "check": true
    },
    {
      "src": "^/submit$",
      "dest": "/api/submit.go",
      "check": true
    },
    {
      "src": "^/api(/.*)?$",
      "status": 404
    },
    {
      "handle": "error"
    },
    {
      "status": 404,
      "src": "^(?!/api).*$",
      "dest": "/404.html"
    },
    {
      "handle": "miss"
    },
    {
      "src": "^/api/(.+)(?:\\.(?:go))$",
      "dest": "/api/$1",
      "check": true
    }
  ]
}
